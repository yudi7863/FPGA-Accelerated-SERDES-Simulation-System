function out_data = gray_encoder(in_data)
    switch in_data
        case [0,0]
            out_data = [0,0];
        case [0,1]
            out_data = [0,1];
        case [1,0]
            out_data = [1,1];
        case [1,1]
            out_data = [1,0];
        otherwise
            disp
    end
end